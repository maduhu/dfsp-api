{
  "implementation": "dfsp-api",
  "cluster": "dfsp1-test",
  "checkLimits": true,
  "console": {
    "port": 30010,
    "server": {
      "host": "0.0.0.0",
      "port": 30010
    }
  },
  "stdOut": {
    "mode": "dev"
  },
  "log": {
    "streams": [{
      "level": "trace",
      "stream": "../logRotateStream",
      "streamConfig": {
        "path": "ut5_%Y-%m-%d.log",
        "symlink": "ut5.log",
        "compress": true
      }
    }]
  },
  "masterBus": {
    "logLevel": "debug",
    "socket": "dfsp-api"
  },
  "workerBus": {
    "logLevel": "debug"
  },
  "db": {
    "logLevel": "trace",
    "db": {
      "host": "postgres"
    },
    "permission": {
      "requireSession": true
    }
  },
  "cache": {
    "storage": "memory",
    "encoding": "json",
    "checkFrequency": 1000,
    "defaultTTL": 3600
  },
  "debug": true,
  "tasks" : {
    "scheduleBulkPayments": {
      "interval": 10
    },
    "_postUsers": {
      "interval": 10
    },
    "_postRules": {
      "interval": 10
    }
  },
  "_forensic": {
    "port": 5678
  }
}
