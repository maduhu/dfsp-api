{
  "implementation": "dfsp-api",
  "repl": false,
  "console": false,
  "log": false,
  "debug": true,
  "workDir": "./tmp",
  "masterBus": {
    "logLevel": "error",
    "socket": "dfsp-api"
  },
  "workerBus": {
    "logLevel": "error"
  },
  "#performance": {
    "id": "dfsp-api",
    "influx": {
      "host": "influxdb",
      "port": 4444
    }
  },
  "#db": {
    "logLevel": "trace",
    "db": {
      "host": "postgres"
    },
    "permission": {
      "requireSession": true
    }
  },
  "ist": {
    "url": "http://localhost:8021"
  },
  "spsp": {
    "url": "http://localhost:8021/spspclient"
  },
  "tasks": {}
}
